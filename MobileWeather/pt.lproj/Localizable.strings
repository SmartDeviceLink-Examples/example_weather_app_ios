/* imperial/metric units */
/* %1$ = "minus" or "", %2$ = formatted value string, %3$ = unit name (with respect to plural rules) */
"units.format" = "%1$@ %2$@ %3$@";

"units.imperial" = "Imperial";
"units.metric" = "Métrico";
"units.negative" = "minus";
"units.positive" = "plus";

"units.temp.degree.short" = "°";
"units.temp.degree.full" = "grau(s)";
